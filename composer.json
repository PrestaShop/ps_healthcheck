{
  "name": "prestashop/ps_healthcheck",
  "description": "PrestaShop - Health check",
  "homepage": "https://github.com/PrestaShop/ps_healthcheck",
  "license": "AFL-3.0",
  "authors": [
    {
      "name": "PrestaShop SA",
      "email": "contact@prestashop.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "laminas/laminas-diagnostics": "^1.6"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "prestashop/php-coding-standards": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "PrestaShop\\Module\\HealthCheck\\": "src/"
    },
    "classmap": ["ps_healthcheck.php"],
    "exclude-from-classmap": []
  },
  "config": {
    "preferred-install": "dist"
  },
  "scripts": {
    "lint": [
      "php-cs-fixer fix --no-interaction --dry-run --diff"
    ]
  },
  "type": "prestashop-module"
}
